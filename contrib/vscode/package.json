{
  "name": "vscode-es-beautifier",
  "displayName": "es-beautifier",
  "publisher": "dai-shi",
  "description": "vscode plugin for es-beautifier",
  "icon": "logo1.svg",
  "version": "0.1.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/es-beautifier.git"
  },
  "dependencies": {
    "es-beautifier": "^0.6.0"
  },
  "devDependencies": {
    "vscode": "^1.0.3"
  },
  "activationEvents": [
    "onLanguage:es-beautifier"
  ],
  "main": "./index.js",
  "contributes": {
    "languages": [{
      "id": "es-beautifier",
      "aliases": ["JavaScript (es-beautifier)"],
      "extensions": ["js", "jsx"]
    }],
    "configuration": {
      "javascript.format.enable": true
    }
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "javascript",
    "emcascript",
    "eslint",
    "beautifier",
    "beautify"
  ],
  "engines": {
    "vscode": "^1.0.0"
  },
  "eslintConfig": {
    "settings": {
      "import/core-modules": [
        "vscode"
      ]
    }
  },
  "license": "MIT"
}
